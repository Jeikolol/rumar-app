<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:RumarApp">

    <Color x:Key="gold-300">#FFE066</Color>
    <Color x:Key="gold-500">#D4AF37</Color>
    <Color x:Key="gold-700">#B8860B</Color>
    <Color x:Key="black">#FF000000</Color>

    <SolidColorBrush x:Key="Gold300Brush" Color="{StaticResource gold-300}" />
    <SolidColorBrush x:Key="Gold500Brush" Color="{StaticResource gold-500}" />
    <SolidColorBrush x:Key="Gold700Brush" Color="{StaticResource gold-700}" />
    <SolidColorBrush x:Key="BlackBrush" Color="{StaticResource black}" />

</ResourceDictionary>

